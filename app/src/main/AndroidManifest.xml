<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ddns1002.triangle">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_triangle"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_triangle"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".SetExerciseActivity"></activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".RoutineActivity" />
        <activity android:name=".ExerciseActivity" />
        <activity android:name=".SelectExerciseActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>